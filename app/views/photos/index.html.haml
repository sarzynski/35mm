- @photos.each do |f|
  .white
    = image_tag f.file.url(:medium)
    - if current_user
      - if current_user.id == f.user_id || current_user.admin == true
        = button_to "Delete", photo_path(f),  :class=>"inbox-sf-add-btn tip", method: :delete
    = f.camera
    %p= f.film
.pag
  = paginate @photos
